{
  "name": "agents-lab",
  "version": "1.0.0",
  "type": "module",
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "// --------- Development ---------": "Development",
    "dev": "tsx watch src/index.ts",
    "dev:debug": "tsx --inspect src/index.ts",
    "dev:profile": "tsx --prof src/index.ts",
    "// Building": "Building",
    "build": "tsc -p tsconfig.json",
    "build:watch": "tsc -w",
    "build:prod": "tsc -p tsconfig.prod.json",
    "// --------- Testing ---------": "Testing",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "jest --testMatch='**/*.integration.test.ts'",
    "test:e2e": "jest --testMatch='**/*.e2e.test.ts'",
    "// --------- Quality ---------": "Quality",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write 'src/**/*.ts'",
    "typecheck": "tsc --noEmit",
    "// --------- Utilities ---------": "Utilities",
    "clean": "rm -rf dist coverage .cache",
    "deps:check": "npm-check-updates",
    "deps:update": "npm-check-updates -u",
    "// --------- Agent-specific ---------": "Agent-specific",
    "agent:chat": "tsx src/cli/chat.ts",
    "agent:tool": "tsx src/cli/tool-runner.ts",
    "ingest": "tsx src/rag/ingest.ts",
    "eval": "tsx src/eval/run-eval.ts",
    "// --------- Monitoring ---------": "Monitoring",
    "monitor": "tsx src/monitoring/dashboard.ts",
    "trace": "tsx src/monitoring/trace-viewer.ts"
  },
  "dependencies": {
    "@langchain/anthropic": "^0.1.0",
    "@langchain/community": "^0.0.40",
    "@langchain/core": "^0.1.50",
    "@langchain/langgraph": "^0.0.20",
    "@langchain/openai": "^0.0.25",
    "chalk": "^5.3.0",
    "dotenv": "^16.4.0",
    "inquirer": "^9.2.0",
    "langchain": "^0.1.30",
    "langsmith": "^0.1.20",
    "openai": "^4.30.0",
    "ora": "^7.0.0",
    "p-queue": "^8.0.0",
    "p-retry": "^6.0.0",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^20.11.0",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.0",
    "npm-check-updates": "^16.14.0",
    "prettier": "^3.2.0",
    "ts-jest": "^29.1.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0"
  },
  "packageManager": "yarn@4.9.2+sha512.1fc009bc09d13cfd0e19efa44cbfc2b9cf6ca61482725eb35bbc5e257e093ebf4130db6dfe15d604ff4b79efd8e1e8e99b25fa7d0a6197c9f9826358d4d65c3c"
}
